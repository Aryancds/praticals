q1
ALTER TALE it_books
ADD column numofpages number(3);

q2
update it_books set numofpages=350 where book_code=1;
update it_books set numofpages=500 where book_code=2;
update it_books set numofpages=550 where book_code=3;
update it_books set numofpages=1000 where book_code=4;
update it_books set numofpages=680 where book_code=5;

q3
ALTER TABLE it_author
MODIFY author_name varchar2(50);

q4
ALTER TABLE it_books
DROP COLUMN edition;

q6
SELECT * FROM it_books



q7
SELECT * FROM it_author;




q8
SELECT * FROM it_books
WHERE book_code(1,2,3,4);

q9
SELECT author_id,author_name from it_author where author_name like'_a%';


q10
update it_books set book_price=850 where publisher='vikash' and page>600;


q11
DROP author_code set it_author where author_name like'_2%';








