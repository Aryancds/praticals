CREATE TABLE it_books
(book_code varchar2(3) PRIMARY KEY,
book_title varchar2(20),
book_price number(10,2),
publisher varchar2(20),
edition varchar2(10));

CREATE TABLE it_author
(author_id varchar2(3) PRIMARY KEY,
author_name varchar2(20),
book_id varchar2(3));

