d a t e o f p r a c t i c a l a s s i g n m e n t : 03


create table it_comp (comp_code varchar(3), comp_name varchar2(20)); 

create table it_product (prod_code varchar(3), prod_name varchar2(20), prod_type varchar2(30), quantity number(10,2), Rate decimal(10,2),
comp_code varchar2(3));

1 . I N S E R T A T L E A S T 5 R E C O R D S I N c o m p a n y t a b le a n d 1 0
r e c o r d s in p r o d u c t t a b le

>>


insert into it_comp values (101,'Nestle');
insert into it_comp values (102,'Parle');
insert into it_comp values (103,'Acer');
insert into it_comp values (104,'Lenovo');
insert into it_comp values (105,'Dell');



insert into it_product values ('p01','Monaco','Biscuit',50,10,102);
insert into it_product values ('p02','CPU','Electronics',10,12000,105);
insert into it_product values ('p03','Keyboard','Electronics',5,1000,103);
insert into it_product values ('p04','Mouse','Electronics',20,500,105);
insert into it_product values ('p05','Coffee','Grocery',20,56,101);




2 . D is p la y o n ly p r o d c o d e a n d p r o d n a m e o f p r o d u c t t a b le

>>
select prod_code, prod_name from it_product;



3 . D is p la y a ll t h e r e c o r d s o f c o m p a n y n a m e w h o s e c o m p a n y c o d e
is b e t w e e n ‘101 ’ t o ‘103 ’

>>
select * from it_comp where comp_code between 101 and 103;




4 . D is p la y p r o d u c t n a m e w h o s e q u a n t it y is le s s t h e n 5 0

>>
select prod_name from it_product where quantity<50;




5 . D is p la y p r o d u c t n a m e , q u a n t it y a n d r a t e w h o s e p r o d u c t n a m e
is s t a r t e d w it h ‘C ’ c h a r a c t e r a n d r a t e is m o r e t h a n 5 0 0 0


>>
select prod_name, quantity, rate from it_product where prod_name like ('C%') and rate>5000;


